service vendor.vppservice /vendor/bin/vppservice
    class hal
    user media
    group camera system

    # HIDL interfaces
    interface vendor.qti.hardware.vpp@2.0::IHidlVppService vppService

    # don't auto restart
    oneshot

    # start only when requested (uncomment below to disable service at boot)
    # disabled
